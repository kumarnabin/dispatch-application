enum Status {
    OPEN("Open")
    WAITING_FOR_RESPONSE("Waiting for Customer Response")
    CLOSED("Closed")
    IN_PROGRESS("In Progress")
    REOPENED("Reopened")
    CANNOT_REPRODUCE("Cannot Reproduce")
    SOLVED("Solved")
    WONT_IMPLEMENT("Won't Implement")
    VERIFIED("Verified")

}
