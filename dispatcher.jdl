// Define the Author entity
entity ServiceProvider {
    name String,
    code String,
    phone String,
    address String,
    status Status,
}

// Define the Book entity with a many-to-one relationship to Author
entity Branch {
    name String,
    code String,
    status Status,
}
entity Olt {
    name String,
    detail String,
    status Status,
}


entity Area {
    name String,
    code String unique,
    detail String,
    status Status,
}

entity Team {
    teamNo String,
    supervisor String,
    supervisorPhoneNo String,
    teamLeader String,
    teamLeaderPhone String
}

entity Dispatch {
    voice String,
    data String,
    iptv String,
    customerName String,
    contactNo String,
    oltPort String,
    regDate String,
    fapPort String,
    cpeSn String,
    cpeRx String,
    complain String,
    remark String,
    status Status,
    location String,
    printDate LocalDate,
    publicationDate Instant
}

entity Employee {
    fullName String,
    dob Date,
    gender String,
    mobile String,
    photo ImageBlob, // Assuming 'photo' is an image file
    citizenshipNo String,
    //    citizenship ImageBlob, // Assuming 'citizenshipFile' is a file
    panNo String,
    //    pan ImageBlob, // Assuming 'panFile' is a file
    category String,
    detail String,
    status Status
}

entity Document {
    name String,
    file ImageBlob,
}
entity EmployeeArea {
    status Status,
    publicationDate Instant
}
entity Attendance {
    status Status,
    meterPics String,
    publicationDate Instant
}
entity DispatchRecord {
    remark String,
    status Status,
    publicationDate Instant
}

relationship OneToOne {
    Employee{user} to User with builtInEntity
}

// Create a many-to-one relationship from Book to Author
relationship ManyToOne {
    Branch{serviceProvider} to ServiceProvider
    Olt{branch} to Branch
    Area{olt} to Olt
    Dispatch{team} to Team
    DispatchRecord{dispatch} to Dispatch
    Document{employee} to Employee
    EmployeeArea{employee} to Employee
    Attendance{employee} to Employee
    DispatchRecord{employee} to Employee
}

//relationship ManyToMany {
//    Area to Employee
//}

// Specify the DTO (Data Transfer Object) for the Book entity
dto * with mapstruct

// Specify the service for the ServiceProvider entity
service * with serviceClass

paginate all with pagination

