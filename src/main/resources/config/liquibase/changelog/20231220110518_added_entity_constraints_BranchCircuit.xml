                                 />

        <addForeignKeyConstraint baseColumnNames="branch_id"
                                 baseTableName="rel_branch_circuit__branch"
                                 constraintName="fk_rel_branch_circuit__branch__branch_id"
                                 referencedColumnNames="id"
                                 referencedTableName="branch"
                                 />
    </changeSet>
</databaseChangeLog>
